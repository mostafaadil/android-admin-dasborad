<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ss.bottomnavsample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.DefaultStyleDynamicActivity"
            android:label="Bottom Navigation" />
        <activity android:name=".activities.SStyleDynamicActivity" />
        <activity
            android:name=".activities.MainActivity"
            android:label="Material Bottom Navigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.DefaultStyleFixedActivity" />
        <activity android:name=".activities.SStyleFixedActivity"></activity>
    </application>

</manifest>